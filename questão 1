#include <stdio.h>
int main() {
 float x, y, z;
 x = 7 + 3*6/2-1;
 printf("primeira operação: multiplicação 3 * 6 = 18\nsegunda operação: divisão 18 / 2 = 9\nterceira operação: soma 7 + 9 = 16 \nquarta operação: subtração 16 - 1 = 15");
printf ("\nx = %.2f",x);
printf("\n\nprimeira operação: resto de 2 em 2 = 0\nsegunda operação: multplicação 2 * 2 = 4\nterceira operação: divisão 2 / 2 = 1\nquarta operação: subtração 4 - 1 = 3\nquinta operação: soma 0 + 3 = 3");
y = 2%2 + 2*2-2/2;
printf ("\ny = %.2f",y);
printf("\n\nprimeira operação: multiplicação 9 * 3 = 27\nsegunda operação: divisão 27 / 3 = 9\nterceira operação: soma 3 + 9 = 12\nquarta operação: multplicação 3 * 9 * 12 = 324 \n");
z = (3 * 9 * (3 + (9*3/ (3)))); 
printf ("\nz = %.2f",z);
return 0;
}
